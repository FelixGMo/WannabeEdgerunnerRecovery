              "secondaryKey": "Mod-Edg-Unconscious-Dividend"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Die maximale Anzahl an Menschlichkeit, die durch einen Knockout regeneriert wird.",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Unconscious-Dividend-Desc"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Exponent",
