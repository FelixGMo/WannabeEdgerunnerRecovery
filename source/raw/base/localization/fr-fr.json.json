          "entries": [
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Régénération d'humanité",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Recovery"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Taux de récupération",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Recovery-Rate"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "La quantité d'humanité récupérée par jour, si aucun Cyberware n'est équipé. Un Cyberware équipé réduit le taux de récupération proportionnellement au nombre de d'emplacement d'inventaire remplis. De sorte qu'aucune humanité n'est récupérée, si tous les emplacements sont remplis.",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Recovery-Rate-Desc"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Seuil Cyberware",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Recovery-Threshold"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "La charge du Cyberware à laquelle aucune humanité n'est récupérée. Toute charge supérieure dégrade l'humanité, et toute charge inférieure régénère l'humanité. La charge est calculée comme le nombre de créneaux remplis divisé par le nombre de créneaux disponibles.",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Recovery-Threshold-Desc"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Knock-out dividend",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Unconscious-Dividend"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Le nombre maximum d'humanité restaurée par un knock-out.",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Unconscious-Dividend-Desc"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Knock-out exponent",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Unconscious-Exponent"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Exposant négatif appliqué à la peine de mort.",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Unconscious-Exponent-Desc"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Régénération en combat",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Recovery-InCombat"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "In-Combat Regeneration commute si l'humanité est régénérée en combat sur la durée.",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Recovery-InCombat-Desc"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Taux adaptatif",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Recovery-Adaptive"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Adaptive rate permet de déterminer si le taux de régénération est dynamique ou statique. Si dynamique, le taux de régénération est calculé proportionnellement à la charge du cyberware ; sinon, le taux de régénération maximal est régénéré ou dégénéré.",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Recovery-Adaptive-Desc"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Récompenses de l'humanité",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Reward"
            },
            {
              "$type": "localizationPersistenceOnScreenEntry",
              "femaleVariant": "Récompense l'humanité pour une action spécifique. Par exemple, donner de l'argent ou caresser Nibbles.",
              "maleVariant": "",
              "primaryKey": 0,
              "secondaryKey": "Mod-Edg-Reward-Desc"
